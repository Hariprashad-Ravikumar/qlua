L = qcd.lattice{4,4,4,4}

function format_tab(t)
  local r = "{"
  local s = ""
  for i = 1, #t do
      r = r .. s .. tostring(t[i])
      s = " "
  end
  return r .. "}"
end
  
io.stdout:write("lattice " .. tostring(L) .. "\n")

ms, nt = qcd.network()

io.stdout:write("network[" .. tostring(ms) .. "]: ".. format_tab(nt) .. "\n")

io.stdout:write("end\n")